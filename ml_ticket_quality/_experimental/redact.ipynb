    "import re\n",
    "\n",
    "def redact_pii(text):\n",
    "    text = re.sub(r'\\b\\d{6}[-+]\\d{4}\\b', '[REDACTED SSN]', text)\n",
    "    text = re.sub(r'\\+46[\\s\\d]{7,}', '[REDACTED PHONE]', text)\n",
    "    text = re.sub(r'\\b[\\w\\.-]+@[\\w\\.-]+\\.\\w+\\b', '[REDACTED EMAIL]', text)\n",
    "    return text"
   ]
  }
